class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        i = 0
        j = len(height) - 1
        max_a = 0
        while i < j:
            h = min(height[i], height[j])
            b = j - i
            a = b * h
            max_a = max(max_a, a)
            if height[i] <= height[j]:
                i += 1
            else:
                j -= 1
        return max_a 
        
