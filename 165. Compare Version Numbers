class Solution(object):
    def compareVersion(self, version1, version2):
        """
        :type version1: str
        :type version2: str
        :rtype: int
        """
        st1 = version1.split('.')
        st2 = version2.split('.')
        n = max(len(st1), len(st2))
        for i in range(n):
            ch1 = int(st1[i]) if i < len(st1) else 0
            ch2 = int(st2[i]) if i < len(st2) else 0
            if ch1 > ch2:
                return 1
            elif ch1 < ch2:
                return -1
        return 0
        
