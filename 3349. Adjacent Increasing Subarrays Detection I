class Solution:
    def hasIncreasingSubarrays(self, nums: List[int], k: int) -> bool:
        def check(num):
            for i in range(1, len(num)):
                if num[i] <= num[i - 1]:
                    return False
            return True
        for i in range(len(nums) - 2*k + 1):
                b = i + k
                if check(nums[i:i+k]) and check(nums[b:b+k]):
                    return True 
        return False
        
